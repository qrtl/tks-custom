<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="account_move_line_form_inherit" model="ir.ui.view">
        <field name="name">account.move.line.form.inherited</field>
        <field name="model">account.move.line</field>
        <field name="inherit_id" ref="account.view_move_line_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='account_id']" position="after">
                <field name="analytic_type_id"/>
            </xpath>
        </field>
    </record>
    
    <record id="account_move_line_tree_inherit" model="ir.ui.view">
        <field name="name">account.move.line.tree.inherited</field>
        <field name="model">account.move.line</field>
        <field name="inherit_id" ref="account.view_move_line_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='analytic_account_id']" position="after">
                <field name="analytic_type_id"/>
            </xpath>
        </field>
    </record>
    
    <record id="account_move_form_inherit" model="ir.ui.view">
        <field name="name">account.move.form.inherited</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='analytic_account_id']" position="after">
                <field name="analytic_type_id"/>
            </xpath>
        </field>
    </record>
</odoo>